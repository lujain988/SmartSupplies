SELECT [o].[ID] AS [Id], [o].[OrderDate], [o].[TotalAmount], [o].[Status], [o].[LoyaltyPoints], [o].[Quantity], [p].[ProductName], [p].[Price], [u].[Username], (
    SELECT TOP(1) [v].[Code]
    FROM [Vouchers] AS [v]
    WHERE [v].[OrderID] = [o].[ID]) AS [Code], (
    SELECT TOP(1) [v0].[DiscountAmount]
    FROM [Vouchers] AS [v0]
    WHERE [v0].[OrderID] = [o].[ID]) AS [DiscountAmount]
FROM [Orders] AS [o]
LEFT JOIN [Products] AS [p] ON [o].[ProductId] = [p].[ID]
LEFT JOIN [Users] AS [u] ON [o].[UserID] = [u].[ID]
WHERE [o].[UserID] = @__id_0
