SELECT [p].[ID] AS [id], [p].[ProductName] AS [productName], [p].[Description] AS [description], [p].[Price] AS [price], [p].[StockQuantity] AS [stockQuantity], [p].[Image] AS [image], [p].[Discount] AS [discount], [c].[CategoryName] AS [categoryName], CAST([t].[Rating] AS decimal(18,2)) AS [reviewRate], [t].[Comment] AS [comment], [t].[Status] AS [status]
FROM [Products] AS [p]
INNER JOIN [Categories] AS [c] ON [p].[CategoryID] = [c].[ID]
OUTER APPLY (
    SELECT [r].[Comment], [r].[Rating], [r].[Status]
    FROM [Reviews] AS [r]
